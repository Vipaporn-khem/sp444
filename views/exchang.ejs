<form class="job-search-form row pt40" action="/inputexchang" method="get">
 <!-- Start of category input -->
 <div class="col-md-2 col-sm-12 search-categories">
    <label for="search-categories">Category</label>
    <select name="rate" class="selectpicker" id="search-categories"  title="select" data-size="5"
        data-container="body" >
        <option value="US">US</option>
        <option value="EU">EU</option>
    </select>
</div>
    <!-- Start of keywords input -->
    <div class="col-md-3 col-sm-12 search-keywords">
        <label for="search-keywords">Keywords</label>
        <input type="number" name="rate" id="search-keywords" placeholder="exchang" style="background-color:rgb(243, 231, 231)">
    
   
</div>

    <!-- Start of submit input -->
    <div class="col-md-2 col-sm-12 search-submit">
        <button type="button" id="convert-ex" class="btn btn-blue btn-effect btn-large"><i
                class="fa fa-search"></i>convert</button>
    </div>

    <!-- Start of keywords input -->
    <div class="col-md-3 col-sm-12 search-keywords">
        <label for="search-keywords">Keywords</label>
        <input type="number" name="thb" id="thb" placeholder="Thaibath" style="background-color:rgb(243, 231, 231)">
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.js"
        integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>
    <script>
        $('#convert-ex').on('click', function () {
            $('#thb').val($('#search-categories').val() == 'US' ? $('#search-keywords').val() * 30.02 : $('#search-keywords').val() * 36.60);
        })
    </script>

